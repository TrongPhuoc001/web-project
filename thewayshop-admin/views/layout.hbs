<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="Dashboard">
    <meta name="keyword" content="Dashboard, Bootstrap, Admin, Template, Theme, Responsive, Fluid, Retina">

    <title>{{title}}</title>

    <!-- Bootstrap core stylesheets -->
    <link href="/stylesheets/bootstrap.css" rel="stylesheet">
    <!--external stylesheets-->
    <link href="/font-awesome/css/font-awesome.css" rel="stylesheet" />
    <link rel="stylesheet" type="text/stylesheets" href="/stylesheets/zabuto_calendar.css">
    <link rel="stylesheet" type="text/stylesheets" href="/javascripts/gritter/css/jquery.gritter.css" />
    <link rel="stylesheet" type="text/stylesheets" href="/lineicons/style.css">    
    
    <!-- Custom styles for this template -->
    <link href="/stylesheets/style.css" rel="stylesheet">
    <link href="/stylesheets/style-responsive.css" rel="stylesheet">

    <script src="/javascripts/chart-master/Chart.js"></script>
    
    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
      <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
  </head>
  <body>
      <section id="container">
           <header class="header black-bg">
              <div class="sidebar-toggle-box">
                  <div class="fa fa-bars tooltips" data-placement="right" data-original-title="Toggle Navigation"></div>
              </div>
            <!--logo start-->
            <a href="/" class="logo"><b>TheWayShop Admin</b></a>
            <!--logo end-->
            <div class="nav notify-row" id="top_menu">
                <!--  notification start -->
                <ul class="nav top-menu">
                    <!-- settings start -->
                    <li class="dropdown">
                        <a data-toggle="dropdown" class="dropdown-toggle" href="index.html#">
                            <i class="fa fa-tasks"></i>
                            <span class="badge bg-theme">{{tasks.length}}</span>
                        </a>
                        <ul class="dropdown-menu extended tasks-bar">
                            <div class="notify-arrow notify-arrow-green"></div>
                            {{#if tasks}}
                            <li>
                            <p class="green">You have {{tasks.length}} pending tasks</p>
                            </li>
                            {{#each tasks}}
                            <li>
                                <a href="/">
                                    <div class="task-info">
                                        <div class="desc">{{this.title}}</div>
                                        <div class="percent">{{this.percent}}</div>
                                    </div>
                                    <div class="progress progress-striped">
                                        <div class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="{{this.percent}}" aria-valuemin="0" aria-valuemax="100" style="width: {{this.percent}}">
                                            <span class="sr-only">{{this.percent}} Complete (success)</span>
                                        </div>
                                    </div>
                                </a>
                            </li>
                            {{/each}}
                            {{else}}
                            <li>
                                <p class="green">No messages</p>
                            </li>
                            {{/if}}
                            <li class="external">
                                <a href="#">See All Tasks</a>
                            </li>
                        </ul>
                    </li>
                    <!-- settings end -->
                    <!-- inbox dropdown start-->
                    <li id="header_inbox_bar" class="dropdown">
                        <a data-toggle="dropdown" class="dropdown-toggle" href="index.html#">
                            <i class="fa fa-envelope-o"></i>
                            <span class="badge bg-theme">{{user_messages.length}}</span>
                        </a>
                        <ul class="dropdown-menu extended inbox">
                            <div class="notify-arrow notify-arrow-green"></div>
                            {{#if user_messages}}
                            <li>
                            <p class="green">You have {{user_messages.length}} new messages</p>
                            </li>
                            {{#each user_messages}}
                            <li>
                                <a href="/">
                                    <span class="photo"><img alt="avatar" src="/images/ui-zac.jpg"></span>
                                    <span class="subject">
                                    <span class="from">{{this.name}}</span>
                                    <span class="time">{{this.time}}</span>
                                    </span>
                                    <span class="message">
                                        {{this.message}}
                                    </span>
                                </a>
                            </li>
                            {{/each}}
                            {{else}}
                            <li>
                                <p class="green">No messages</p>
                            </li>
                            {{/if}}
                            <li>
                                <a href="/">See all messages</a>
                            </li>
                        </ul>
                    </li>
                    <!-- inbox dropdown end -->
                </ul>
                <!--  notification end -->
            </div>
            <div class="top-menu">
            	<ul class="nav pull-right top-menu">
                    <li><a class="logout" href="/users/logout">Logout</a></li>
            	</ul>
            </div>
        </header>
        <!--header end-->
        
        <!--sidebar start-->
        <aside>
            <div id="sidebar"  class="nav-collapse ">
                <!-- sidebar menu start-->
                <ul class="sidebar-menu" id="nav-accordion">
                
                    <p class="centered"><a href="/profile"><img src="/images/ui-sam.jpg" class="img-circle" width="60"></a></p>
                    <h5 class="centered">{{username}}</h5>
                        
                    <li class="mt">
                        <a class="active" href="/">
                            <i class="fa fa-dashboard"></i>
                            <span>Dashboard</span>
                        </a>
                    </li>
                    <li class="sub-menu">
                        <a href="javascript:;" >
                            <i class="fa fa-th"></i>
                            <span>Data Tables</span>
                        </a>
                        <ul class="sub">
                            {{#each tables}}
                            <li><a  href="/tables/{{this}}">{{this}}</a></li>
                            {{/each}}
                        </ul>
                    </li>
                </ul>
                <!-- sidebar menu end-->
            </div>
        </aside>
        {{{body}}}
      </section>
    
     <!-- javascripts placed at the end of the document so the pages load faster -->
    <script src="/javascripts/jquery.js"></script>
    <script src="/javascripts/jquery-1.8.3.min.js"></script>
    <script src="/javascripts/bootstrap.min.js"></script>
    <script class="include" type="text/javascript" src="/javascripts/jquery.dcjqaccordion.2.7.js"></script>
    <script src="/javascripts/jquery.scrollTo.min.js"></script>
    <script src="/javascripts/jquery.nicescroll.js" type="text/javascript"></script>
    <script src="/javascripts/jquery.sparkline.js"></script>


    <!--common script for all pages-->
    <script src="/javascripts/common-scripts.js"></script>
    
    <script type="text/javascript" src="/javascripts/gritter/js/jquery.gritter.js"></script>
    <script type="text/javascript" src="/javascripts/gritter-conf.js"></script>

    <!--script for this page-->
    <script src="/javascripts/sparkline-chart.js"></script>    
	<script src="/javascripts/zabuto_calendar.js"></script>	
	
  </body>