                            <h3>Salary: {{manager.salary}}$</h3>
                        </div>
                    </div>
                </div>
            </div><!-- col-lg-12-->      	
        </div><!-- /row -->
    </section><! --/wrapper -->
</section><!-- /MAIN CONTENT -->

