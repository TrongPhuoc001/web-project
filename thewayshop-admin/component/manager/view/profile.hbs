
<!--main content start-->
<section id="main-content">
    <section class="wrapper">
        <!-- BASIC FORM ELELEMNTS -->
        <div class="row mt">
            <div class="col-lg-12">
                <div class="form-panel">
                    <h4 class="mb"><i class="fa fa-angle-right"></i> {{manager.fullname}}</h4>
                    <div class="row">
                        <div class="col-lg-4">
                            <img {{#if this.image}}src="{{this.image}}"{{else}}src="/images/friends/fr-06.jpg"{{/if}} class="img-circle" width="80">
                        </div>
                        <div class="col-lg-8">
                            <h3>Username: {{manager.username}}</h3>
